{
  "name": "org-build",
  "version": "0.86.0",
  "description": "Support package for building/packaging apps",
  "keywords": [
    "webpack",
    "typescript"
  ],
  "author": "Matt Penrice <matthew.penrice@gmail.com>",
  "homepage": "https://github.com/elmpp/partridge#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmpp/partridge.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "prepare": "cd ../../ && yarn prepare",
    "transpile": "npm run build",
    "transpile:watch": "npm run build -- --watch",
    "type-check": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/elmpp/partridge/issues"
  },
  "bin": {
    "orgBuild": "./dist/bin/org-build.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "7.4.5",
    "@babel/preset-flow": "7.0.0",
    "@types/copy": "^0.3.1",
    "@types/fs-extra": "^8.0.0",
    "@types/lodash-webpack-plugin": "^0.11.3",
    "@types/react-dev-utils": "^9.0.1",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/webpack": "4.32.1",
    "@types/webpack-dev-server": "^3.1.7",
    "@types/webpack-env": "^1.14.0",
    "@types/webpack-node-externals": "^1.6.3",
    "@types/webpackbar": "^2.6.0",
    "copy": "^0.3.2",
    "debug": "4.1.1",
    "flow-remove-types": "2.106.1",
    "fs-extra": "^8.1.0",
    "get-dev-paths": "0.1.1",
    "lodash-webpack-plugin": "^0.11.5",
    "org-common": "^0.86.0",
    "partridge-config": "^0.86.0",
    "razzle-dev-utils": "^3.0.0",
    "react-dev-utils": "^9.0.3",
    "start-server-webpack-plugin": "^2.2.5",
    "terser-webpack-plugin": "^1.4.1",
    "typescript": "3.6.4",
    "webpack": "4.34.0",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^4.0.0"
  }
}
