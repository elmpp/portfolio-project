{
  "name": "partridge-logging",
  "version": "0.86.0",
  "description": "Provides logging services to other packages",
  "keywords": [
    "logging"
  ],
  "author": "Matt Penrice <matthew.penrice@gmail.com>",
  "homepage": "https://github.com/elmpp/partridge#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmpp/partridge-logging.git"
  },
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "npm run type-check",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "prepare": "cd ../../ && yarn prepare",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "transpile": "npm run build",
    "transpile:watch": "npm run build -- --watch",
    "type-check": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/elmpp/partridge/issues"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^0.10.2",
    "chalk": "2.4.2",
    "debug": "4.1.1",
    "lodash": "4.17.15",
    "org-common": "^0.86.0",
    "partridge-config": "^0.86.0",
    "triple-beam": "^1.3.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/triple-beam": "^1.3.0",
    "@types/winston": "^2.4.4",
    "stackdriver-errors-js": "^0.7.0",
    "typescript": "3.6.4"
  }
}
