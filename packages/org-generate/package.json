{
  "name": "org-generate",
  "version": "0.86.0",
  "description": "code templating and generation",
  "author": "\"Matt Penrice\" <\"matthew.penrice@gmail.com\">",
  "homepage": "https://github.com/elmpp/partridge#readme",
  "license": "ISC",
  "main": "lib/org-generate.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmpp/partridge.git"
  },
  "bin": {
    "generate": "scripts/generate.sh"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "plop": "plop",
    "prepare": "cd ../../ && yarn prepare",
    "transpile": "npm run build",
    "transpile:watch": "npm run build -- --watch",
    "type-check": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/elmpp/partridge/issues"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "inquirer-fuzzy-path": "^2.1.0",
    "plop": "^2.4.0"
  },
  "dependencies": {
    "lodash": "4.17.15"
  }
}
